<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
            <% if (orders.length > 0) { %>

                <ul>
                    <% for (let order of orders) { %>
                        <h1># <%= order.id %></h1>
                        <ul>
                            <% for (product of order.products) { %>
                                <li>
                                    <%= product.title %> (<%= product.orderItem.quantity %>)
                                </li>
                            <% } %>
                        </ul>
                    <% } %>
                </ul>

            <% } else { %>
                <h1>Nothing there!</h1>
            <% } %>
        </main>
        <%- include('../includes/end.ejs') %>